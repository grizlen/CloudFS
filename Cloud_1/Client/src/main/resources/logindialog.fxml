<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="16.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fx.LoginController">
   <HBox spacing="8.0">
      <Label text="Name" />
      <TextField HBox.hgrow="ALWAYS" />
   </HBox>
   <HBox spacing="8.0">
      <Label text="Password" />
      <TextField HBox.hgrow="ALWAYS" />
   </HBox>
   <HBox spacing="8.0">
      <Button text="OK" />
      <Button text="Cancel" />
   </HBox>
   <padding>
      <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
   </padding>
</VBox>
